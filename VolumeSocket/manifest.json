{
	"manifest_version": 3,
	"name": "WebSocket Volume Control",
	"description": "Bidirectional volume control for YouTube pages",
	"version": "0.0.4",
	"content_scripts": [{
		"world": "MAIN",
		"exclude_globs": ["https://dashboard.twitch.tv/*"],
		"matches": ["https://*.youtube.com/*", "file:///*", "https://*.twitch.tv/*"],
		"js": ["volsock.js"]
	}]
}
